{
  "name": "@context-engine/core",
  "version": "0.1.0",
  "description": "Shared types, schemas, and utilities for Context Engine",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js",
      "require": "./dist/config/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./extractor": {
      "types": "./dist/extractor/index.d.ts",
      "import": "./dist/extractor/index.js",
      "require": "./dist/extractor/index.cjs"
    },
    "./generator": {
      "types": "./dist/generator/index.d.ts",
      "import": "./dist/generator/index.js",
      "require": "./dist/generator/index.cjs"
    },
    "./manifest": {
      "types": "./dist/manifest/index.d.ts",
      "import": "./dist/manifest/index.js",
      "require": "./dist/manifest/index.cjs"
    },
    "./processor": {
      "types": "./dist/processor/index.d.ts",
      "import": "./dist/processor/index.js",
      "require": "./dist/processor/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "process:help": "tsx scripts/run-processor.ts --help",
    "extract": "tsx scripts/run-processor.ts --phase extract",
    "generate": "tsx scripts/run-processor.ts --phase generate",
    "manifest": "tsx scripts/run-processor.ts --phase build",
    "process": "tsx scripts/run-processor.ts --phase full"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@google/genai": "^1.39.0",
    "@types/lodash": "^4.17.23",
    "fast-json-stable-stringify": "^2.1.0",
    "lodash": "^4.17.23",
    "react-docgen-typescript": "^2.4.0",
    "semver": "^7.7.3",
    "ts-morph": "^27.0.2",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/semver": "^7.7.1",
    "@vitest/ui": "^4.0.18",
    "dotenv": "^17.2.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist"
  ],
  "license": "AGPL-3.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
